{"_id": "527d5fe3c431431eee7a8645", "categories": {"audience": [], "database": [], "developmentstatus": [], "environment": [], "language": [], "license": [{"fullname": "Creative Commons Attribution License", "fullpath": "License :: Creative Commons Attribution License", "id": 868, "shortname": "ccal"}], "os": [], "topic": [], "translation": []}, "creation_date": "2013-11-08", "developers": [{"name": "Aristeidis Telonis", "url": "http://sourceforge.net/u/atelonis/", "username": "atelonis"}], "external_homepage": null, "icon_url": "http://sourceforge.net/p/plasmiddesigner/icon", "labels": [], "moved_to_url": "", "name": "plasmiddesigner", "preferred_support_tool": "_members", "preferred_support_url": "", "private": false, "screenshots": [{"caption": "Main Window of the application", "thumbnail_url": "http://sourceforge.net/p/plasmiddesigner/screenshot/MainWindow.png/thumb", "url": "http://sourceforge.net/p/plasmiddesigner/screenshot/MainWindow.png"}, {"caption": "Dialog to configure Feature characteristics", "thumbnail_url": "http://sourceforge.net/p/plasmiddesigner/screenshot/Configure.png/thumb", "url": "http://sourceforge.net/p/plasmiddesigner/screenshot/Configure.png"}], "short_description": "Plasmid designer (PlaNer) is a software developed to easily create plasmid maps and extract the figure of it. Features, that is 'genes' (i.e. sequences of interest) or 'sites' (e.g. restriction sites), can be added, removed or deleted at any time. For every Feature, a different colour can be determined for appropriate visualization. \r\nThe list of the features can be saved at any time and an existing one can be loaded to continue configuring a previously saved plasmid map. PlaNer uses the easy-to-handle txt file type for this purpose. \r\nThe image of the plasmid map consists of the actual map and a list of the colour-coded names of the specific features on the right. This can be extracted and saved as a png image at any time.\r\n\r\nFor support, questions, ideas or feedback:\r\natelonis@users.sourceforge.net", "shortname": "plasmiddesigner", "socialnetworks": [{"accounturl": "", "socialnetwork": "Twitter"}, {"accounturl": null, "socialnetwork": "Facebook"}], "status": "active", "summary": "Software for drawing plasmid map", "tools": [{"label": "Files", "mount_point": "files", "name": "files"}, {"label": "Support", "mount_point": "support", "name": "support"}, {"label": "Reviews", "mount_point": "reviews", "name": "reviews"}, {"label": "Summary", "mount_point": "summary", "name": "summary", "sourceforge_group_id": 2002217}, {"label": "Wiki", "mount_point": "wiki", "name": "wiki"}, {"label": "Code", "mount_point": "code", "name": "git"}, {"label": "Tickets", "mount_point": "tickets", "name": "tickets"}, {"label": "Discussion", "mount_point": "discussion", "name": "discussion"}, {"label": "Activity", "mount_point": "activity", "name": "activity"}], "url": "http://sourceforge.net/p/plasmiddesigner/", "video_url": ""}
