{"_id": "5139e5b0a02bb10b2fd30660", "categories": {"audience": [], "database": [], "developmentstatus": [], "environment": [], "language": [], "license": [], "os": [], "topic": [], "translation": []}, "creation_date": "2013-03-08", "developers": [{"name": "Adrian Noguero", "url": "http://sourceforge.net/u/anoguero/", "username": "anoguero"}], "external_homepage": null, "icon_url": "http://sourceforge.net/p/lehiatila/icon", "labels": [], "moved_to_url": "", "name": "Leihatila", "preferred_support_tool": "discussion", "preferred_support_url": "", "private": false, "screenshots": [{"caption": "Users management", "thumbnail_url": "http://sourceforge.net/p/lehiatila/screenshot/Leihatila-GestionUsuarios.jpg/thumb", "url": "http://sourceforge.net/p/lehiatila/screenshot/Leihatila-GestionUsuarios.jpg"}, {"caption": "Manage Local Currency", "thumbnail_url": "http://sourceforge.net/p/lehiatila/screenshot/Leihatila-SeleccionOperacion.jpg/thumb", "url": "http://sourceforge.net/p/lehiatila/screenshot/Leihatila-SeleccionOperacion.jpg"}, {"caption": "Confirmation", "thumbnail_url": "http://sourceforge.net/p/lehiatila/screenshot/Leihatila-ConfirmacionCompra.jpg/thumb", "url": "http://sourceforge.net/p/lehiatila/screenshot/Leihatila-ConfirmacionCompra.jpg"}, {"caption": "Full System View", "thumbnail_url": "http://sourceforge.net/p/lehiatila/screenshot/Leihatila-VistaDeSistema.jpg/thumb", "url": "http://sourceforge.net/p/lehiatila/screenshot/Leihatila-VistaDeSistema.jpg"}], "short_description": "Leihatila (small window or counter, in basque) is an application to manage a local currency system backed by the official currency. \r\n\r\nLehiatila allows to deploy exchange offices for local currency systems where, new users can be created and the flows of money in your local network can be controlled (buying, redeeming and expiration transactions are supported). It has been prepared to work with notes that include barcodes to control serial numbers.\r\n\r\nLehiatila will be used by the Ekhi Txanpona project (www.ekhitxanpona.org) in Bilbao, Spain.\r\n", "shortname": "lehiatila", "socialnetworks": [{"accounturl": "@ekhitxanpona", "socialnetwork": "Twitter"}, {"accounturl": "http://www.facebook.com/ekhitxanpona", "socialnetwork": "Facebook"}], "status": "active", "summary": "Local currency management software (not LETS)", "tools": [{"label": "Blog", "mount_point": "blog", "name": "blog"}, {"label": "Files", "mount_point": "files", "name": "files"}, {"label": "Wiki", "mount_point": "wiki", "name": "wiki"}, {"label": "Discussion", "mount_point": "discussion", "name": "discussion"}, {"label": "Code", "mount_point": "code", "name": "svn"}, {"label": "Tickets", "mount_point": "tickets", "name": "tickets"}, {"label": "Activity", "mount_point": "activity", "name": "activity"}, {"label": "Summary", "mount_point": "summary", "name": "summary", "sourceforge_group_id": 1344245}, {"label": "Reviews", "mount_point": "reviews", "name": "reviews"}, {"label": "Support", "mount_point": "support", "name": "support"}], "url": "http://sourceforge.net/p/lehiatila/", "video_url": ""}
