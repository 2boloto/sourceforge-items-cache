{"_id": "52adcd70485acd20c9d46c96", "categories": {"audience": [], "database": [], "developmentstatus": [], "environment": [], "language": [], "license": [], "os": [], "topic": [], "translation": []}, "creation_date": "2013-12-15", "developers": [{"name": "Lia Di", "url": "http://sourceforge.net/u/dilie/", "username": "dilie"}, {"name": "William McCaig", "url": "http://sourceforge.net/u/wmccaig/", "username": "wmccaig"}, {"name": "Daniel Packer", "url": "http://sourceforge.net/u/dpacker/", "username": "dpacker"}, {"name": "Weigang Qiu", "url": "http://sourceforge.net/u/wgqiu/", "username": "wgqiu"}], "external_homepage": null, "icon_url": null, "labels": [], "moved_to_url": "", "name": "phyloBrowse", "preferred_support_tool": "", "preferred_support_url": "", "private": false, "screenshots": [{"caption": "Visual identification of synonymous and nonsynonymous substitutions", "thumbnail_url": "http://sourceforge.net/p/phylobrowse/screenshot/borreliabase-screenshot-1.png/thumb", "url": "http://sourceforge.net/p/phylobrowse/screenshot/borreliabase-screenshot-1.png"}], "short_description": "Existing genome browsers (gBrowser, jBrowser) are designed to explore a single genome. Features to facilitate comparisons among genomes are often retrofitted as an afterthought. But, as evolutionary biologists like to quote T. Dobzansky, \"Nothing in biology makes sense except in the light of evolution\". To paraphrase this, \"nothing in genomics makes sense except in the light of phylogeny\". A phylo-centric genome broswer creates evolutionary expecations and allows, for example, visual identification of hypervariable genes, conserved regulatory sequences, and recombination events.", "shortname": "phylobrowse", "socialnetworks": [{"accounturl": "", "socialnetwork": "Twitter"}, {"accounturl": null, "socialnetwork": "Facebook"}], "status": "active", "summary": "This project implements a phylogeny-centered genome browswer", "tools": [{"label": "Files", "mount_point": "files", "name": "fi