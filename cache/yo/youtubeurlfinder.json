{"_id": "54428039bcf63a2019ed8abd", "categories": {"audience": [{"fullname": "End Users/Desktop", "fullpath": "Intended Audience :: by End-User Class :: End Users/Desktop", "id": 2, "shortname": "endusers"}], "database": [], "developmentstatus": [{"fullname": "5 - Production/Stable", "fullpath": "Development Status :: 5 - Production/Stable", "id": 11, "shortname": "production"}], "environment": [{"fullname": "Web-based", "fullpath": "User Interface :: Web-based", "id": 237, "shortname": "web"}], "language": [{"fullname": "JavaScript", "fullpath": "Programming Language :: JavaScript", "id": 280, "shortname": "JavaScript"}], "license": [{"fullname": "Creative Commons Attribution License", "fullpath": "License :: Creative Commons Attribution License", "id": 868, "shortname": "ccal"}], "os": [], "topic": [{"fullname": "WWW/HTTP", "fullpath": "Topic :: Internet :: WWW/HTTP", "id": 90, "shortname": "www"}], "translation": []}, "creation_date": "2014-10-18", "developers": [{"name": "M. Aris Zamroni", "url": "http://sourceforge.net/u/maz1980/", "username": "maz1980"}], "external_homepage": "http://youtubeurlfinder.sourceforge.net", "icon_url": "http://sourceforge.net/p/youtubeurlfinder/icon", "labels": [""], "moved_to_url": "", "name": "Youtube URL Finder", "preferred_support_tool": "tickets", "preferred_support_url": "", "private": false, "screenshots": [{"caption": "", "thumbnail_url": "http://sourceforge.net/p/youtubeurlfinder/screenshot/YUF.png/thumb", "url": "http://sourceforge.net/p/youtubeurlfinder/screenshot/YUF.png"}], "short_description": "This is a single HTML file containing Javascript codes to generates URL of video, audio and subtitle objects of Youtube video. You can can directly open or save it from browser. The generated URLs can also be pasted into download accelerator to have multi threaded download.\r\n\r\nThe program generates URL of Youtube's format:\r\n1. Non-DASH URLs contains video & audio.\r\n2. DASH URLs contains video only or audio only.\r\n3. Closed caption or subtitle in SRT format.\r\nRead more about DASH in section \"Comparison of YouTube media encoding options\" of http://en.wikipedia.org/wiki/YouTube\r\n\r\nTo play DASH MP4 files in players other than VLC, repack the files using GPAC MP4BOX tool:\r\na. Create iTunes m4a: mp4box -add dash_audio.mp4#1 -new itunesfile.m4a\r\nb. Create standard MP4 video: mp4box -add dash_video.mp4#1 -add dash_audio.mp4#1 -new standard.mp4\r\n", "shortname": "youtubeurlfinder", "socialnetworks": [{"accounturl": "", "socialnetwork": "Twitter"}, {"accounturl": "https://www.facebook.com/ma.zamroni", "socialnetwork": "Facebook"}], "status": "active", "summary": "HTML & Javascript based Youtube's video URL finder", "tools": [{"label": "Wiki", "mount_point": "wiki", "name": "wiki"}, {"label": "Activity", "mount_point": "activity", "name": "activity"}, {"label": "Blog", "mount_point": "blog", "name": "blog"}, {"label": "Discussion", "mount_point": "discussion", "name": "discussion"}, {"label": "Files", "mount_point": "files", "name": "files"}, {"label": "Summary", "mount_point": "summary", "name": "summary", "sourceforge_group_id": 2346878}, {"label": "Support", "mount_point": "support", "name": "support"}, {"label": "Reviews", "mount_point": "reviews", "name": "reviews"}, {"label": "Tickets", "mount_point": "tickets", "name": "tickets"}, {"label": "Code", "mount_point": "code", "name": "git"}], "url": "http://sourceforge.net/p/youtubeurlfinder/", "video_url": null}
