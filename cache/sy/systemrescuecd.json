{"_id": "50eb2e58e88f3d24ce5c0688", "categories": {"audience": [], "database": [], "developmentstatus": [], "environment": [], "language": [], "license": [], "os": [], "topic": [], "translation": []}, "creation_date": "2003-07-16", "developers": [{"name": "Gregory Nowak", "url": "http://sourceforge.net/u/gnowak/", "username": "gnowak"}, {"name": "Jean-Fran\u00e7ois Tissoires", "url": "http://sourceforge.net/u/tissoires/", "username": "tissoires"}, {"name": "Pierre Dorgueil", "url": "http://sourceforge.net/u/pdorgueil/", "username": "pdorgueil"}, {"name": "Daniel Biehle", "url": "http://sourceforge.net/u/tuxbrain/", "username": "tuxbrain"}, {"name": "Franck Ladurelle", "url": "http://sourceforge.net/u/ladurelf/", "username": "ladurelf"}, {"name": "Francois Dupoux", "url": "http://sourceforge.net/u/fdupoux/", "username": "fdupoux"}], "external_homepage": "http://www.sysresccd.org/", "icon_url": "http://sourceforge.net/p/systemrescuecd/icon", "labels": [], "moved_to_url": "", "name": "SystemRescueCd", "preferred_support_tool": "", "preferred_support_url": "", "private": false, "screenshots": [{"caption": "SystemRescueCd: graphical tools", "thumbnail_url": "http://sourceforge.net/p/systemrescuecd/screenshot/259296.jpg/thumb", "url": "http://sourceforge.net/p/systemrescuecd/screenshot/259296.jpg"}, {"caption": "SystemRescueCd: first screen", "thumbnail_url": "http://sourceforge.net/p/systemrescuecd/screenshot/259298.jpg/thumb", "url": "http://sourceforge.net/p/systemrescuecd/screenshot/259298.jpg"}, {"caption": "SystemRescueCd: graphical environment", "thumbnail_url": "http://sourceforge.net/p/systemrescuecd/screenshot/259294.jpg/thumb", "url": "http://sourceforge.net/p/systemrescuecd/screenshot/259294.jpg"}], "short_description": "SystemRescueCd is a Linux system rescue disk available as a bootable CD-ROM or USB stick for administrating or repairing your system and data after a crash. It aims to provide an easy way to carry out admin tasks on your computer, such as creating and editing the hard disk partitions. \r\n\r\nIt comes with a lot of software such as disk management tools (parted, partimage, fsarchiver, filesystem tools, ...), network administration programs and simple text editors . It can be used for both Linux and windows computers, and on desktops as well as servers. \r\n\r\nThis rescue system requires no installation as it can be booted from a CD/DVD drive, and USB stick, or from the network using PXE. But it can be installed on the hard disk if you wish. It comes with up to date kernels to provide support for recent hardware and also for all important file systems (ext2/ext3/ext4, xfs, btrfs, ntfs, reiserfs, vfat), as well as network filesystems (samba and nfs).", "shortname": "systemrescuecd", "socialnetworks": [{"accounturl": "", "socialnetwork": "Twitter"}, {"accounturl": null, "socialnetwork": "Facebook"}], "status": "active", "summary": "A Linux system rescue disk as a bootable CD-ROM or USB stick", "tools": [{"label": "Summary", "mount_point": "summary", "name": "summary", "sourceforge_group_id": 85811}, {"label": "Wiki", "mount_point": "wiki", "name": "wiki"}, {"label": "Files", "mount_point": "files", "name": "files"}, {"label": "Mailing Lists", "mount_point": "mailman", "name": "mailman"}, {"label": "Code", "mount_point": "code", "name": "git"}, {"label": "Reviews", "mount_point": "reviews", "name": "reviews"}, {"label": "Support", "mount_point": "support", "name": "support"}, {"label": "Activity", "mount_point": "activity", "name": "activity"}], "url": "http://sourceforge.net/p/systemrescuecd/", "video_url": null}
