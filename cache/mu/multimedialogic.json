{"_id": "51108ede1be1ce3b4d534da3", "categories": {"audience": [], "database": [], "developmentstatus": [], "environment": [], "language": [], "license": [], "os": [], "topic": [], "translation": []}, "creation_date": "2013-02-05", "developers": [{"name": "Jason Kresowaty", "url": "http://sourceforge.net/u/jasonkres/", "username": "jasonkres"}, {"name": "James Larson", "url": "http://sourceforge.net/u/jlarson43/", "username": "jlarson43"}, {"name": "Robert Yaklin", "url": "http://sourceforge.net/u/yakl/", "username": "yakl"}], "external_homepage": "http://sourceforge.net/projects/multimedialogic/", "icon_url": "http://sourceforge.net/p/multimedialogic/icon", "labels": [], "moved_to_url": "", "name": "MultiMedia Logic", "preferred_support_tool": "tickets", "preferred_support_url": "", "private": false, "screenshots": [{"caption": "A peek at help", "thumbnail_url": "http://sourceforge.net/p/multimedialogic/screenshot/mmlogicHelp.png/thumb", "url": "http://sourceforge.net/p/multimedialogic/screenshot/mmlogicHelp.png"}, {"caption": "Just finished playing the Binary Tutor theme song in MultiMedia Logic", "thumbnail_url": "http://sourceforge.net/p/multimedialogic/screenshot/BinaryTutorSong.png/thumb", "url": "http://sourceforge.net/p/multimedialogic/screenshot/BinaryTutorSong.png"}, {"caption": "An example of a line following robot.", "thumbnail_url": "http://sourceforge.net/p/multimedialogic/screenshot/RobotExample.png/thumb", "url": "http://sourceforge.net/p/multimedialogic/screenshot/RobotExample.png"}], "short_description": "Welcome to one of the most simplest, most powerful, most universal languages known (Digital Logic). Most digital logic drawing systems are just that, drawing systems. Recently, some logic drawing systems allow circuits to be activated as they are drawn for testing purposes. But the input and output to these systems are typically files of numbers. The MultiMedia Logic Simulator has taken this one step further and introduced devices that connect directly to your computers\u2019 real devices (e.g. Keyboard, Screen, Serial Ports) including MultiMedia ones (PC speaker, Wave, Bitmaps).\r\n\r\nThe intent of this system is not to necessarily build the logic circuit you design. The intent is to use what you build, to allow experimentation, to learn and to have fun.\r\n", "shortname": "multimedialogic", "socialnetworks": [{"accounturl": "", "socialnetwork": "Twitter"}], "status": "active", "summary": "A MultiMedia Logic Design System", "tools": [{"label": "Files", "mount_point": "files", "name": "files"}, {"label": "Code", "mount_point": "code", "name": "git"}, {"label": "Tickets", "mount_point": "tickets", "name": "tickets"}, {"label": "Discussion", "mount_point": "discussion", "name": "discussion"}, {"label": "Summary", "mount_point": "summary", "name": "summary", "sourceforge_group_id": 1270350}, {"label": "Reviews", "mount_point": "reviews", "name": "reviews"}, {"label": "Support", "mount_point": "support", "name": "support"}, {"label": "Activity", "mount_point": "activity", "name": "activity"}], "url": "http://sourceforge.net/p/multimedialogic/", "video_url": ""}
