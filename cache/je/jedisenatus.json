tion_date": "2013-06-06", "developers": [{"name": "David Visicchio", "url": "http://sourceforge.net/u/visidavid/", "username": "visidavid"}, {"name": "Alberto Persello", "url": "http://sourceforge.net/u/alperse73/", "username": "alperse73"}, {"name": "Ciro D'Urso", "url": "http://sourceforge.net/u/cirodurso/", "username": "cirodurso"}], "external_homepage": "http://jedisenatus.sourceforge.net", "icon_url": "http://sourceforge.net/p/jedisenatus/icon", "labels": ["Data Integration", "Report Generator", "Data Export", "XML", "PDF", "EXCEL", "RTF", "FOP"], "moved_to_url": "", "name": "Jedi", "preferred_support_tool": "_members", "preferred_support_url": "", "private": false, "screenshots": [{"caption": "General Description", "thumbnail_url": "http://sourceforge.net/p/jedisenatus/screenshot/General_description.gif/thumb", "url": "http://sourceforge.net/p/jedisenatus/screenshot/General_description.gif"}, {"caption": "Handling request", "thumbnail_url": "http://sourceforge.net/p/jedisenatus/screenshot/Handling_request.jpg/thumb", "url": "http://sourceforge.net/p/jedisenatus/screenshot/Handling_request.jpg"}], "short_description": "JEDI is a J2EE application that provides a centralized service aiming at significantly simplify the generation of data driven documents in a enterprise environment.\r\nThe documents (hereafter called \"JEDI documents\") can have different format types: pdf, excel, rtf, plain text data streams and xml streams.\r\n\r\nA JEDI document is an instance of the so called \"managed document\" (i.e. the configuration data and a particular set of rules), configured by a developer into the JEDI configuration database. The provision of a JEDI document is based on a synchronous processing performed by the JEDI core engine triggered by an end-user request (through HTTP request or RMI call).\r\n\r\nThe content of a JEDI document (i.e. the input of the JEDI service), must be an XML stream regardless of the source. Such data is subsequently processed through one or more XSL transformations in order to provide the user with the document in the required format.\r\n", "shortname": "jedisenatus", "socialnetworks": [{"accounturl": "", "socialnetwork": "Twitter"}], "status": "active", "summary": "Java Enhanced Data Interface - Italian Senate Project", "tools": [{"label": "Summary", "mount_point": "summary", "name": "summary", "sourceforge_group_id": 1859899}, {"label": "Files", "mount_point": "files", "name": "files"}, {"label": "Reviews", "mount_point": "reviews", "name": "reviews"}, {"label": "Support", "mount_point": "support", "name": "support"}, {"label": "Wiki", "mount_point": "wiki", "name": "wiki"}, {"label": "Code", "mount_point": "code", "name": "git"}, {"label": "Discussion", "mount_point": "discussion", "name": "discussion"}, {"label": "Activity", "mount_point": "activity", "name": "activity"}], "url": "http://sourceforge.net/p/jedisenatus/", "video_url": ""}
