{"_id": "516332c404161f50849c2c7f", "categories": {"audience": [], "database": [], "developmentstatus": [], "environment": [], "language": [], "license": [], "os": [], "topic": [], "translation": []}, "creation_date": "2013-04-08", "developers": [{"name": "Simon Ridley", "url": "http://sourceforge.net/u/ridders/", "username": "ridders"}], "external_homepage": "", "icon_url": "http://sourceforge.net/p/warbler-twitter/icon", "labels": [], "moved_to_url": "", "name": "Warbler", "preferred_support_tool": "", "preferred_support_url": "", "private": false, "screenshots": [{"caption": "API RAW data output", "thumbnail_url": "http://sourceforge.net/p/warbler-twitter/screenshot/warbler3.jpg/thumb", "url": "http://sourceforge.net/p/warbler-twitter/screenshot/warbler3.jpg"}, {"caption": "Warbler v1.4", "thumbnail_url": "http://sourceforge.net/p/warbler-twitter/screenshot/warbler1.jpg/thumb", "url": "http://sourceforge.net/p/warbler-twitter/screenshot/warbler1.jpg"}, {"caption": ".TSV file output", "thumbnail_url": "http://sourceforge.net/p/warbler-twitter/screenshot/warbler2.jpg/thumb", "url": "http://sourceforge.net/p/warbler-twitter/screenshot/warbler2.jpg"}], "short_description": "A Ruby based Twitter profile extraction tool, primarily for law enforcement who need to preserve twitter evidence or monitor profiles.\r\n\r\nWarbler can currently provide the following information:\r\n\r\nCreated at\r\nTimezone\t\r\nLocation\t\r\nName\t\r\nScreen Name\t\r\nUser ID\t\r\nText\r\nTweet ID\t\r\nGeo\t\r\nTweet Source\t\r\nIn reply to screen name\t\r\nIn reply to User ID\r\n\r\nThe above is entered into a TSV file for easy reviewing. The script will also provide a TXT file containing the RAW API data should you wish to use other software to review the data.\r\n\r\n***Tool is currently inactive due to Twitters changes to API authentication. Update to follow.***", "shortname": "warbler-twitter", "socialnetworks": [{"accounturl": "simon_ridley", "socialnetwork": "Twitter"}], "status": "abandoned", "summary": "", "tools": [{"label": "Summary", "mount_point": "summary", "name": "summary", "sourceforge_group_id": 1512023}, {"label": "Files", "mount_point": "files", "name": "files"}, {"label": "Reviews", "mount_point": "reviews", "name": "reviews"}, {"label": "Support", "mount_point": "support", "name": "support"}], "url": "http://sourceforge.net/p/warbler-twitter/", "video_url": ""}
