estafonts", "socialnetworks": [], "status": "active", "summary": "Avestan Unicode Fonts", "tools": [{"label": "Wiki", "mount_point": "wiki", "name": "wiki"}, {"label": "Files", "mount_point": "files", "name": "files"}, {"label": "Code", "mount_point": "code", "name": "git"}, {"label": "Tickets", "mount_point": "tickets", "name": "tickets"}, {"label": "Discussion", "mount_point": "discussion", "name": "discussion"}, {"label": "Mailing Lists", "mount_point": "mailman", "name": "mailman"}, {"label": "Summary", "mount_point": "summary", "name": "summary", "sourceforge_group_id": 1173584}, {"label": "Reviews", "mount_point": "reviews", "name": "reviews"}, {"label": "Support", "mount_point": "support", "name": "support"}, {"label": "Activity", "mount_point": "activity", "name": "activity"}], "url": "http://sourceforge.net/p/avestafonts/", "video_url": ""}
