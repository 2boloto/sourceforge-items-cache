{"_id": "4f367c07b9363c5271001138", "categories": {"audience": [], "database": [], "developmentstatus": [{"fullname": "4 - Beta", "fullpath": "Development Status :: 4 - Beta", "id": 10, "shortname": "beta"}], "environment": [{"fullname": "Java Swing", "fullpath": "User Interface :: Graphical :: Java Swing", "id": 471, "shortname": "ui_swing"}], "language": [{"fullname": "Java", "fullpath": "Programming Language :: Java", "id": 198, "shortname": "java"}], "license": [{"fullname": "GNU Library or Lesser General Public License version 2.0 (LGPLv2)", "fullpath": "License :: OSI-Approved Open Source :: GNU Library or Lesser General Public License version 2.0 (LGPLv2)", "id": 16, "shortname": "lgpl"}], "os": [], "topic": [{"fullname": "Electronic Design Automation (EDA)", "fullpath": "Topic :: Scientific/Engineering :: Electronic Design Automation (EDA)", "id": 246, "shortname": "eda"}], "translation": []}, "creation_date": "2012-02-11", "developers": [{"name": "Frank Storm", "url": "http://sourceforge.net/u/fstorm/", "username": "fstorm"}], "external_homepage": null, "icon_url": "http://sourceforge.net/p/belfft/icon", "labels": ["FFT"], "moved_to_url": "", "name": "bel_fft", "preferred_support_tool": "_members", "preferred_support_url": "", "private": false, "screenshots": [{"caption": "Configuration wizard, general tab", "thumbnail_url": "http://sourceforge.net/p/belfft/screenshot/wizard-general.png/thumb", "url": "http://sourceforge.net/p/belfft/screenshot/wizard-general.png"}, {"caption": "Configuration wizard, configuration settings", "thumbnail_url": "http://sourceforge.net/p/belfft/screenshot/wizard-configurations.png/thumb", "url": "http://sourceforge.net/p/belfft/screenshot/wizard-configurations.png"}, {"caption": "Configuration wizard, simulation input data", "thumbnail_url": "http://sourceforge.net/p/belfft/screenshot/wizard-simulationdata.png/thumb", "url": "http://sourceforge.net/p/belfft/screenshot/wizard-simulationdata.png"}, {"caption": "Configuration wizard, target technology Xilinx", "thumbnail_url": "http://sourceforge.net/p/belfft/screenshot/Target-Configuration-Xilinx.png/thumb", "url": "http://sourceforge.net/p/belfft/screenshot/Target-Configuration-Xilinx.png"}, {"caption": "bel_fft, connected to Zynq processor system in Vivado IP-Integrator", "thumbnail_url": "http://sourceforge.net/p/belfft/screenshot/Vivado-IPI-System.png/thumb", "url": "http://sourceforge.net/p/belfft/screenshot/Vivado-IPI-System.png"}], "short_description": "bel_fft is a FFT co-processor that can calculate FFTs with arbitrary radix. It is a hardware implementation of the free software Kiss FFT (\"Keep it simple, Stupid!\"). The target was to allow a simple replacement of the software code with the hardware implementation. Therefore bel_fft comes with a software driver that is compatible with the Kiss FFT routines. bel_fft also has a modular architecture and allows interfacing different bus architectures. So far AMBA AXI, Altera's Avalon bus and the Wishbone bus are supported. However, bel_fft's architecture allows an easy adaptation to further bus architectures (e.g. AMBA AHB). It comes with a Java wizard to configure the co-processor and to generate all required files (e.g. twiddle ROMs).\r\nIntegration into QSYS from Altera, XIlinx EDK and Vivado IP-Integrator is supported as well.\r\nbel_fft is distributed under the GNU Lesser Public License 2.1.\r\n", "shortname": "belfft", "socialnetworks": [{"accounturl": "", "socialnetwork": "Twitter"}, {"accounturl": null, "socialnetwork": "Facebook"}], "status": "active", "summary": "FFT co-processor in Verilog based on the KISS FFT", "tools": [{"label": "Wiki", "mount_point": "wiki", "name": "wiki"}, {"label": "Files", "mount_point": "downloads", "name": "files"}, {"label": "Code", "mount_point": "code", "name": "git"}, {"label": "Discussion", "mount_point": "discussion", "name": "discussion"}, {"label": "Blog", "mount_point": "blog", "name": "blog"}, {"label": "Tickets", "mount_point": "tickets", "name": "tickets"}, {"label": "Activity", "mount_point": "activity", "name": "activity"}, {"label": "Summary", "mount_point": "summary", "name": "summary", "sourceforge_group_id": 689478}, {"label": "Reviews", "mount_point": "reviews", "name": "reviews"}, {"label": "Support", "mount_point": "support", "name": "support"}], "url": "http://sourceforge.net/p/belfft/", "video_url": ""}
