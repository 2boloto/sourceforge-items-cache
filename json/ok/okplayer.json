player", "socialnetworks": [{"accounturl": "", "socialnetwork": "Twitter"}, {"accounturl": null, "socialnetwork": "Facebook"}], "status": "active", "summary": "A simple but powerfull folder-based audio player", "tools": [{"label": "Activity", "mount_point": "activity", "name": "activity"}, {"label": "Summary", "mount_point": "summary", "name": "summary", "sourceforge_group_id": 2412195}, {"label": "Files", "mount_point": "files", "name": "files"}, {"label": "Reviews", "mount_point": "reviews", "name": "reviews"}, {"label": "Support", "mount_point": "support", "name": "support"}, {"label": "Discussion", "mount_point": "discussion", "name": "discussion"}], "url": "http://sourceforge.net/p/okplayer/", "video_url": null}
