{"_id": "54732e71485acd5f0d2bd26b", "categories": {"audience": [], "database": [], "developmentstatus": [], "environment": [], "language": [], "license": [], "os": [], "topic": [], "translation": []}, "creation_date": "2014-11-24", "developers": [{"name": "Clecio Lopes", "url": "http://sourceforge.net/u/clepes/", "username": "clepes"}, {"name": "Jos\u00e9 Paulo Vidal Amaral", "url": "http://sourceforge.net/u/josepva/", "username": "josepva"}], "external_homepage": "http://msxallinone.sourceforge.net", "icon_url": null, "labels": [], "moved_to_url": "", "name": "MSX-ALL-IN-ONE", "preferred_support_tool": "_url", "preferred_support_url": "http://paixaoeletronica.blogspot.com.br/", "private": false, "screenshots": [{"caption": "First Circuit Block Diagram. General Idea of what will be the final circuit.", "thumbnail_url": "http://sourceforge.net/p/msxallinone/screenshot/First%20Block%20Diagram.jpg/thumb", "url": "http://sourceforge.net/p/msxallinone/screenshot/First%20Block%20Diagram.jpg"}], "short_description": "This device is intended to have Memory Mapper, MegaRam, Ethernet, emulated IDE interface using SDCards, FM (and maybe OPL4) support and RS232.\r\nAll this will be accomplished using a single PIC32MZ family chip as the main component of the device. Using the horse power of that MCU we believe that we can emulate several MSX devices at once without any or with few wait states cicles.\r\nThis project is in its preliminaries stages but if we are successful it will became an \"ALL-IN-ONE\" solution for MSX Computers with reduced costs and increased performance.", "shortname": "msxallinone", "socialnetworks": [{"accounturl": "", "socialnetwork": "Twitter"}, {"accounturl": null, "socialnetwork": "Facebook"}], "status": "active", "summary": "It will be a device for MSX Computers comprising of several devices.", "tools": [{"label": "Activity", "mount_point": "activity", "name": "activity"}, {"label": "Files", "mount_point": "files", "name": "files"}, {"label": "Summary